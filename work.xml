<?xml version="1.0"?>
<tryton>
	<data>
		<record model="ir.ui.view" id="work_view_tree">
			<field name="model">timesheet.work</field>
			<field name="inherit" ref="timesheet.work_view_tree"/>
			<field name="arch" type="xml">
				<![CDATA[
				<data>
					<xpath
						expr="/tree/field[@name=&quot;rec_name&quot;]" position="after">
						<field name="billable_hours"/>
					</xpath>
				</data>
				]]>
			</field>
		</record>
		<record model="ir.ui.view" id="work_view_form">
			<field name="model">timesheet.work</field>
			<field name="inherit" ref="timesheet.work_view_form"/>
			<field name="arch" type="xml">
				<![CDATA[
				<data>
					<xpath
						expr="/form/field[@name=&quot;company&quot;]" position="after">
						<label name="billable"/>
						<field name="billable"/>
						<label name="billable_hours"/>
						<field name="billable_hours"/>
					</xpath>
				</data>
				]]>
			</field>
		</record>

	</data>
</tryton>
